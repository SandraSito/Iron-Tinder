<div id="pgeneral-container">
  <div class="nav-profile">
    <a href="/auth/logout">Logout</a>
    <a href="/chat">CHAT</a>
  </div>
  <div id="profile-container">
    <div id="left-panel">
      <div id="user-panel">
        <img id="user-panel-image" src='' alt="">
        <h3 id="user-panel-name"></h3>
      </div>
      <div id="user-panel-matchs">
        {{> matches}}
      </div>
    </div>
    <div id="right-panel">
      <div id="user-panel-candidate">
        {{> candidate}}
      </div>
      <div id="like-dislike">
        <button id="like-botton"><img src="/images/like.png"></button>
        <button id="dislike-botton"><img src="/images/dislike.png"></button>
      </div>
    </div>
  </div>

</div>





<script>
  window.info = {{{ frontInfo }}}
</script>
<script src="/javascripts/tools.js"></script>
